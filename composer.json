{
    "name": "ilovelylong/dingtalk2",
    "description": "EasyDingTalk, Alibaba Dingtalk SDK for PHP",
    "keywords": [
        "dingtalk",
        "dingding",
        "easydingtalk",
        "sdk",
        "钉钉"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "ilovelylong",
            "email": "ilovelylong@gmail.com"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^7.4",
        "php": ">=7.0",
        "pimple/pimple": "^3.0",
        "monolog/monolog": "^1.23 || ^2.0",
        "symfony/http-foundation": "^3.2 || ^4.0 || ^5.0|| ^6.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.5 || ^7.0"
    },
    "autoload": {
        "psr-4": {
            "EasyDingTalk\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "EasyDingTalk\\Tests\\": "tests/"
        }
    }
}
